{-
Returns `True` if an `Integer` is positive or `False` if it is negative or zero.
-}

let Integer/nonPositive =
        ./nonPositive sha256:e00a852eed5b84ff60487097d8aadce53c9e5301f53ff4954044bd68949fac3b
      ? ./nonPositive

let positive
    : Integer → Bool
    = λ(n : Integer) → Integer/nonPositive n == False

let example0 = assert : positive +4 ≡ True

let example1 = assert : positive +0 ≡ False

let example2 = assert : positive -3 ≡ False

in  positive
